<form id="create" name="create" method="post" action="<%=_.url.action('create')%>">
<% data.fields.forEach(function(f) { %>
    <label for="<%=f.name%>"><%=f.name%>:</label>
<%- _.field(f) %>
    <br />
<% }) %>
  <button type="submit" name="_redirect" value="<%=_.url.action('list')%>">Save and Close</button>
  <button type="submit" name="_redirect" value="<%=_.url.action('new')%>">Save and Create Another</button>
</form>